<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <PackageId>EnumGenie.Core</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>Steve Leigh</Authors>
    <Description>Generate enums matching your C# enums.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes />
    <Copyright>Copyright 2016 (c) Steve Leigh. All rights reserved.</Copyright>
    <PackageTags>codegen</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="NETStandard.Library" Version="1.6.1" />
  </ItemGroup>
</Project>